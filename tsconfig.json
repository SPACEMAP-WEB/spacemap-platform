{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@app.components/*": ["src/app.components/*"],
      "@app.layout/*": ["src/app.layout/*"],
      "@app.feature/*": ["src/app.feature/*"],
      "@app.modules/*": ["src/app.modules/*"],
      // "@app.store/*":["src/app.store/*"],
      "@pages/*": ["src/pages/*"],
      "@public/*": ["public/*"],
      "@types/*": ["types/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
